#include "../includes/asm.h"

int	ld_operator(t_env *e, int start, int sl)
{
	start += skip_space(e->stock + e->count + start);
	if (!check_nb_param(e, start, sl, 0) || !valid_param(e, start, 0))
		return (0);
}
